{
  "#type": "merge",
  "values": [
    "$.request",
    {
      "aggregations": {
        "#type": "as_list",
        "values": {
          "#type": "fallback",
          "strategies": [
            "$.request.aggregations",
            {
              "creationDate": {
                "date_histogram": {
                  "field": "creation.date.from",
                  "fixed_interval": " 315360000000ms",
                  "format": "y G || y"
                }
              }
            }
          ]
        }
      }
    },
    "$.request"
  ]
}
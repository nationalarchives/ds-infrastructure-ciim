{
  "results": {
    "request": {
      "query": "$.query",
      "aggregations": "$.aggregations",
      "from": "$.from",
      "size": "$.size",
      "sort": "$.sort"
    },
    "cache": {
      "name": "fast-cache"
    }
  },
  "aggs": {
    "request": {
      "query": {
        "#type": "merge","depth": -1,
        "values": [
          "$.current",
          {
            "query": {
              "bool": {
                "filter": {
                  "#type": "require_catch",
                  "value": {
                    "spec": {
                      "exists": {
                        "field": {
                          "@datatype.group.value": "*"
                        }
                      }
                    }
                  }
                }
              }
            }
          }
        ]
      },
      "aggregations": {
        "group": {
          "terms": {
            "field": "@datatype.group.value",
            "size": "50"
          }
        }
      },
      "size": 0,
      "timeout": "60s"
    },
    "cache": {
      "name": "slow-cache"
    }
  }
}
{
  "results": {
    "request": {
      "query": "$.query",
      "aggregations": "$.aggregations",
      "from": "$.from",
      "size": "$.size",
      "sort": "$.sort"
    },
    "cache": {
      "name": "fast-cache"
    }
  },
  "aggs": {
    "#type": "merge",
    "values": [
      "$.request",
      {
        "query": {
          "bool": {
            "must": [
              "$.request.query",
              {
                "range": {
                  "@admin.sequence": {
                    "gte": "$.original_request.sequence_from",
                    "lte": "$.original_request.sequence_to"
                  }
                }
              },
              {
                "range": {
                  "@admin.sequence": {
                    "gt": "$.original_request.last_sequence"
                  }
                }
              }
            ]
          }
        },
        "aggregations": {
          "group": {
            "terms": {
              "field": "@datatype.group.value",
              "size": "50"
            }
          }
        },
        "from": 0,
        "size": 0
      }
    ],
    "cache": {
      "name": "slow-cache"
    }
  }
}
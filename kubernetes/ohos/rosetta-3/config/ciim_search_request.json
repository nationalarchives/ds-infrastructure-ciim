{
  "results": {
    "request": {
      "query": "$.query",
      "aggregations": {
        "#type": "merge",
        "values": [
          "$.aggregations",
          {
            "creationDateDecade": {
              "date_histogram": {
                "field": "creation.date.from",
                "fixed_interval": " 315360000000ms",
                "format": "y G || y"
              }
            }
          }

        ]
      },
      "from": "$.from",
      "size": "$.size",
      "sort": "$.sort"
    },
    "cache": {
      "name": "fast-cache"
    }
  },
  "aggs": {
    "request": {
      "query": {
          "bool": {
            "filter": [
              {}
            ],
            "must": "$.query.bool.must"
          }
      },
      "aggregations": {
        "group": {
          "terms": {
            "field": "@datatype.group.value",
            "size": "50"
          }
        }
      },
      "size": 0,
      "timeout": "60s"
    },
    "cache": {
      "name": "slow-cache"
    }
  }
}
{
  "#type": "chain",
  "chain": [
    {
      "#type": "merge",
      "values": [
        "$.results.datasets.elasticsearch",
        "$.results.datasets.elasticsearchEtna",
        {
          "stats": "$.results.stats"
        }
      ]
    },
    {
      "data": {
        "#type": "fallback",
        "strategies": [
          {
            "#type": "as_list",
            "values": {
              "#type": "for_each",
              "values": "$.data",
              "spec": {
                "@template": {
                  "details": {
                    "accessCondition": "$.availability.access.condition.value",
                    "accruals": "$.accruals.value",
                    "accumulationDates": "$.accruals.date.value",
                    "administrativeBackground": "$.origination.description[?(@.type == 'administrative background')].value",
                    "agentBirthLocation": {
                      "#type": "as_list",
                      "values": "$.agent[0].@birth.location.name.value"
                    },
                    "agentDateOfBirth": {
                      "#type": "as_list",
                      "values": "$.agent[0].@birth.date.value"
                    },
                    "agentName": {
                      "#type": "as_list",
                      "values": "$.agent[0].name.value"
                    },
                    "agentServiceNumber": {
                      "#type": "as_list",
                      "values": "$.agent[0].service.identifier.value"
                    },
                    "appraisalInformation": "$.appraisal.value",
                    "arrangement": "$.arrangement.value",
                    "catalogueContext":{
                      "#type": "as_list",
                      "values": "$.@template.details.catalogueContext"
                    },
                    "closureStatus": "$.availability.closure.label.value",
                    "copiesInformation": {
                      "#type": "as_list",
                      "values": "$.copies[0].value"
                    },
                    "creator": {
                      "#type": "as_list",
                      "values": "$.@template.details.creator"
                    },
                    "custodialHistory": "$.custodial[*].description.value",
                    "dateCovering": "$.origination.date.value",
                    "deliveryOption": "$.availability.delivery.condition.value",
                    "description": "$.description[0].value",
                    "digitised": "$.@template.details.digitised",
                    "dimensions": "$.measurements.dimensions[*].value",
                    "formerDepartmentReference": "$.identifier[?(@.type == 'former reference (Department)')].value",
                    "formerProReference": "$.identifier[?(@.type == 'former reference (PRO)')].value",
                    "heldBy": "$.repository.name.value",
                    "heldById": "$.repository.@admin.id",
                    "iaid": "$.identifier..__iaid",
                    "immediateSourceOfAcquisition": {
                      "#type": "as_list",
                      "values": "$.@template.details.immediateSourceOfAcquisition"
                    },
                    "language": {
                      "#type": "as_list",
                      "values": "$.language[*].value"
                    },
                    "legalStatus": "$.legal.status",
                    "level": "$.level.value",
                    "locationOfOriginals": {
                      "#type": "as_list",
                      "values": "$.originals[*].organisation.name"
                    },
                    "mapDesignation": "$.@template.details.mapDesignation",
                    "mapScale": "$.@template.details.mapScale",
                    "note": "$.@template.details.note",
                    "physicalCondition": "$.condition.value",
                    "physicalDescription": "$.@template.details.physicalDescription",
                    "primaryIdentifier": "$.@admin.id",
                    "publicationNote": {
                      "#type": "as_list",
                      "values": "$.note[?(@.type == 'publication note')].value"
                    },
                    "recordOpening": "$.availability.access.opening.label.value",
                    "referenceNumber": "$.identifier[?(@.type == 'reference number')].value",
                    "relatedMaterials": {
                      "#type": "as_list",
                      "values":"$.@template.details.relatedMaterials"
                    },
                    "restrictionsOnUse": "$.availability.restrictions.value",
                    "separatedMaterials": "$.@template.details.separatedMaterials",
                    "sourceUrl": "$.@template.details.sourceUrl",
                    "summaryTitle": "$.summary.title",
                    "title": "$.title[?(@.primary == true)].value",
                    "type": "$.@datatype.base",
                    "unpublishedFindingAids": {
                      "#type": "as_list",
                      "values": "$.note[?(@.type == 'unpublished finding aids')].value"
                    },
                    "level": "$.source.level.value",
                    "description": "$.source.description[0].value",
                    "descriptionCy": "$.source.description[?(@.language == 'cy')].value",
                    "date": "$.source.date[0].value",
                    "summary": "$.source.summary.title",
                    "title": "$.source.title[0].value",
                    "titleCy": "$.source.title[?(@.language == 'cy')].value",
                    "itemURL": "$.source.identifier[?(@.type == 'url')].value",
                    "identifier": "$.source.identifier[?(@.type == 'identifier')].value",
                    "uuid": "$.source.@admin.uuid",
                    "creationDateTo": "$.source.creation.date[0].to",
                    "creationDateFrom": "$.source.creation.date[0].from",
                    "creationDate": "$.source.creation.date[0].value",
                    "creator": "$.source.creation.maker[0].name[0].value",
                    "note": "$.source.note[0].value",
                    "collection": "$.source.parent[0].summary.title",
                    "collectionId": "$.source.parent[0].@admin.id",
                    "type": "$.source.name[?(@.type =='type')].value",
                    "format": "$.source.name[?(@.type =='format')].value",
                    "publisher": "$.source.creation.publisher[0].name[0].value",
                    "rights": "$.source.legal.holder",
                    "location": "$.source.place[0].name[0].value",
                    "ciimId": "$.source.@admin.id",
                    "group": "$.source.@datatype.group[0].value",
                    "lon": "$.source.coordinates[0].lon",
                    "lat": "$.source.coordinates[0].lat",
                    "licence": "$.source.licence.value",
                    "status": "$.source.status.value",
                    "accessStatus": "$.source.legal.access.value",
                    "repository": "$.source.repository.code.value",
                    "enrichment": "$.source.enrichment",
                    "subjects": {
                      "#type": "as_list",
                      "values": {
                        "#type": "fallback",
                        "strategies": [
                          "$.source.subject.summary.title",
                          "$.source.subject[*].summary.title",
                          "$.source.tag[*].value"
                        ]
                      }
                    }
                  }
                }
              }
            }
          },
          []
        ]
      },
      "errors": {
        "#type": "fallback",
        "strategies": [
          {
            "#type": "as_list",
            "values": {
              "#type": "flatten",
              "values": [
                "$$.results.errors",
                "$$.aggs.errors",
                "$$.results.datasets.elasticsearch.errors",
                "$$.aggs.datasets.elasticsearch.errors"
              ]
            }
          },
          []
        ]
      },
      "aggregations": {
        "#type": "fallback",
        "strategies": [
          {
            "#type": "as_list",
            "values": {
              "#type": "flatten",
              "values": [
                "$$.aggs.datasets.elasticsearch.aggregations",
                "$$.aggs.datasets.elasticsearchEtna.aggregations"
              ]
            }
          },
          []
        ]
      },
      "stats": {
        "total": "$.stats.total",
        "results": "$$.results.stats.results",
        "providers": "$.stats.providers",
        "latency": {
          "#type": "chain",
          "chain": [
            {
              "#type": "as_list",
              "values": [
                "$$.results.stats.latency",
                "$$.aggs.stats.latency"
              ]
            },
            "$.sum()"
          ]
        }
      },
      "found": "$.found"
    }
  ]
}
  "type_registry":{
    "es_query":{
      "#type":"http_request",
      "url":"{%.search_url}",
      "method":"POST",
      "headers":{
        "Content-Type":"application/json",
        "Authorization": "Basic ZWxhc3RpYzozMjNSV005alU1WFoyTWtuMTkwbHJlOEI="
      },

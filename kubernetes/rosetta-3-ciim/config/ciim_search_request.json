{
  "results": {
    "request": {
      "track_total_hits": true,
      "query": "$.query",
      "aggregations": "$.aggregations",
      "from": "$.from",
      "size": "$.size",
      "sort": "$.sort",
      "_source:": {
        "includes": [
          "summary.title",
          "title",
          "description",
          "identifier",
          "origination.date.value",
          "repository.name.value",
          "@template.details.digitised",
          "multimedia.@admin.id",
          "subjects.subject.summary.title",
          "level",
          "@datatype.group"
        ]
      }
    },
    "cache": {
      "name": "fast-cache"
    }
  },
  "aggs": {
    "request": {
      "query": {
        "bool": {
          "must": "$.query.bool.must"
        }
      },
      "aggregations": {
        "group": {
          "terms": {
            "field": "@datatype.group.value",
            "size": "50"
          }
        }
      },
      "size": 0,
      "timeout": "60s"
    },
    "cache": {
      "name": "slow-cache"
    }
  }
}
{
  "results": {
    "request": {
      "query": "$.query",
      "aggregations": {
        "#type": "merge",
        "values": [
          "$.aggregations",
          {
            "#type": "for_each",
            "values": {
              "#type": "entries",
              "map": "$.aggregations"
            },
            "spec": {
              "$.key": {
                "#type": "merge",
                "values": [
                  "$.value",
                  {
                    "terms": {
                      "min_doc_count": 0
                    }
                  }
                ]
              }
            }
          }
        ]
      },
      "from": "$.from",
      "size": "$.size",
      "sort": "$.sort"
    },
    "cache": {
      "name": "fast-cache"
    }
  },
  "aggs": {
    "request": {
      "query": "$.query",
      "aggregations": {
        "#type": "merge",
        "values": [
          "$.aggregations",
          {
            "#type": "for_each",
            "values": {
              "#type": "entries",
              "map": "$.aggregations"
            },
            "spec": {
              "$.key": {
                "#type": "merge",
                "values": [
                  "$.value",
                  {
                    "terms": {
                      "min_doc_count": 0
                    }
                  }
                ]
              }
            }
          }
        ]
      },
      "from": "$.from",
      "size": 0,
      "timeout": "60s"
    },
    "cache": {
      "name": "slow-cache"
    }
  }

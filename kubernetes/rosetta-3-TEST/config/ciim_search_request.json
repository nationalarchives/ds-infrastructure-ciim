{
  "results": {
    "request": {
      "query": "$.query",
      "aggregations": {
        "#type": "for_each",
        "values": "$.aggregations",
        "spec": {
          "$.name": {
            "#type": "as_list",
            "values": {
              "#type": "for_each",
              "values": "$.entries"
            }
          }
        }
      },
      "from": "$.from",
      "size": "$.size",
      "sort": "$.sort"
    },
    "cache": {
      "name": "fast-cache"
    }
  },
  "aggs": {
    "request": {
      "query": {
        "bool": {
          "must": "$.query.bool.must"
        }
      },
      "aggregations": {
        "group": {
          "terms": {
            "field": "@datatype.group.value",
            "size": "50"
          }
        }
      },
      "size": 0,
      "timeout": "60s"
    },
    "cache": {
      "name": "slow-cache"
    }
  }
}
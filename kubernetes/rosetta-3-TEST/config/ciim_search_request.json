{
  "results": {
    "request": {
      "query": "$.query",
      "aggregations": {
        "#type": "merge",
        "values": [
          "$.aggregations",
          {
            "#type": "for_each",
            "values": {
              "#type": "chain",
              "chain": [
                {
                  "#type": "entries",
                  "map": "$.aggregations"
                },
                "$[?(@.value.terms)]"
              ]
            },
            "spec": {
              "$.key": {
                "#type": "merge",
                "depth": -1,
                "values": [
                  "$.value",
                  {
                    "terms": {
                      "min_doc_count": 0
                    }
                  }
                ]
              }
            }
          }
        ]
      },
      "from": "$.from",
      "size": "$.size",
      "sort": "$.sort"
    },
    "cache": {
      "name": "fast-cache"
    }
  },
  "aggs": {
    "request": {
      "query": {
        "bool": {
          "must": "$.query.bool.must"
        }
      },
      "aggregations": {
        "group": {
          "terms": {
            "field": "@datatype.group.value",
            "size": "50"
          }
        }
      },
      "size": 0,
      "timeout": "60s"
    },
    "cache": {
      "name": "slow-cache"
    }
  }
}

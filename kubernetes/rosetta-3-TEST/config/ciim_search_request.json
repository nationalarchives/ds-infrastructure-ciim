{
  "results": {
    "request": {
      "query": "$.query",
      "aggregations": {
        "#type": "for_each",
        "values": "$.aggregations",
        "spec": {
          "#type": "merge",
          "values": [
            "$",
            {
              "#type": "merge",
              "values": [
                "$.terms",
                {
                  "min_doc_count": 0
                }
              ],
              "#path": "terms"
            }
          ]
        }
      },
      "from": "$.from",
      "size": "$.size",
      "sort": "$.sort"
    },
    "cache": {
      "name": "fast-cache"
    }
  },
  "aggs": {
    "request": {
      "query": {
        "bool": {
          "must": "$.query.bool.must"
        }
      },
      "aggregations": {
        "group": {
          "terms": {
            "field": "@datatype.group.value",
            "size": "50"
          }
        }
      },
      "size": 0,
      "timeout": "60s"
    },
    "cache": {
      "name": "slow-cache"
    }
  }
}
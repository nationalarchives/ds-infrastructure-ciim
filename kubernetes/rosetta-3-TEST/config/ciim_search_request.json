{
  "results": {
    "request": {
      "query": "$.query",
      "aggregations": "$.aggregations",
      "from": "$.from",
      "size": "$.size",
      "sort": "$.sort",
      "highlight": {
        "fields": {
          "name.value": {},
          "identifier.__reference_number": {},
          "agent.name.value": {},
          "agent.summary.title.keyword": {},
          "title.value.keyword": {},
          "origination.creator.name.value": {},
          "description.value": {},
          "origination.description.value": {},
          "custodial.description.value": {},
          "originals.description.value": {}
        }
      }
    },
    "cache": {
      "name": "fast-cache"
    }
  },
  "aggs": {
    "request": {
      "query": {
          "bool": {
            "must": "$.query.bool.must"
          }
      },
      "aggregations": {
        "group": {
          "terms": {
            "field": "@datatype.group.value",
            "size": "50"
          }
        }
      },
      "size": 0,
      "timeout": "60s"
    },
    "cache": {
      "name": "slow-cache"
    }
  }
}
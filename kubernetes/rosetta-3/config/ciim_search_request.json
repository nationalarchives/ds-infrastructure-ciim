{
  "results": {
    "request": {
      "query": "$.query",
      "aggregations": "$.aggregations",
      "from": "$.from",
      "size": "$.size",
      "sort": "$.sort",
      "highlight": {
        "pre_tags": ["<em>"],
        "post_tags": ["</em>"],
        "fields": {
          "_generic_all_std": {},
          "_all": {}
        }
      }
    },
    "cache": {
      "name": "fast-cache"
    }
  },
  "aggs": {
    "request": {
      "query": {
          "bool": {
            "must": "$.query.bool.must"
          }
      },
      "aggregations": {
        "group": {
          "terms": {
            "field": "@datatype.group.value",
            "size": "50"
          }
        }
      },
      "size": 0,
      "timeout": "60s"
    },
    "cache": {
      "name": "slow-cache"
    }
  }
}